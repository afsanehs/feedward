<!-- Project Card Example -->
              <div class="card shadow mb-4">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary">Projects</h6>
                </div>
                <div class="card-body">
                  <h4 class="small font-weight-bold">Note globale de l'entreprise <span class="float-right"><%=@score_global_average%></span></h4>
                  <div class="progress mb-4">
                    <% if @score_global_average == nil %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: 0%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_global_average <= 1.5 %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: <%=@score_global_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_global_average > 1.5 && @score_global_average <= 3.5 %>
                      <div class="progress-bar bg-warning" role="progressbar" style="width: <%=@score_global_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% else %>
                      <div class="progress-bar bg-success" role="progressbar" style="width: <%=@score_global_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% end %>
                  </div>
                
                  <h4 class="small font-weight-bold">Votre note <span class="float-right"><%=@score_global_average_by_user%></span></h4>
                  <div class="progress mb-4">
                    <% if @score_global_average_by_user == nil %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: 0%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_global_average_by_user <= 1.5 %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: <%=@score_global_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_global_average_by_user > 1.5 && @score_global_average_by_user <= 3.5 %>
                      <div class="progress-bar bg-warning" role="progressbar" style="width: <%=@score_global_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% else %>
                      <div class="progress-bar bg-success" role="progressbar" style="width: <%=@score_global_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% end %>
                  </div>

                  <h4 class="small font-weight-bold">Note de l'environnement de travail de l'entreprise <span class="float-right"><%=@score_workspace_average%></span></h4>
                  <div class="progress mb-4">
                    <% if @score_workspace_average == nil %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: 0%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_workspace_average <= 1.5 %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: <%=@score_workspace_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_workspace_average > 1.5 && @score_workspace_average <= 3.5 %>
                      <div class="progress-bar bg-warning" role="progressbar" style="width: <%=@score_workspace_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% else %>
                      <div class="progress-bar bg-success" role="progressbar" style="width: <%=@score_workspace_average/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% end %>
                  </div>

                  <h4 class="small font-weight-bold">Votre note <span class="float-right"><%=@score_workspace_average_by_user%></span></h4>
                  <div class="progress mb-4">
                    <% if @score_workspace_average_by_user == nil %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: 0%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_workspace_average_by_user <= 1.5 %>
                      <div class="progress-bar bg-danger" role="progressbar" style="width: <%=@score_workspace_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% elsif @score_workspace_average_by_user > 1.5 && @score_workspace_average_by_user <= 3.5 %>
                      <div class="progress-bar bg-warning" role="progressbar" style="width: <%=@score_workspace_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% else %>
                      <div class="progress-bar bg-success" role="progressbar" style="width: <%=@score_workspace_average_by_user/5*100%>%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                    <% end %>
                  </div>

                  <h4 class="small font-weight-bold">Note des missions de l'entreprise<span class="float-right"><%=@score_missions_average%></span></h4>

                  
                  <div class="progress">
                    <div class="progress-bar bg-success" role="progressbar" style="width: <%=@score_missions_average/5*100%>%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <h4 class="small font-weight-bold">Votre note <span class="float-right"><%=@score_missions_average_by_user%></span></h4>
                  <div class="progress">
                    <div class="progress-bar bg-success" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                </div>
              </div>