<!-- Approach -->
<div class="col-6 feedback-send-received-section">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-indigo text-uppercase">Feedbacks reçus</h6>
        </div>
        <div class="card-body">

            <% @feedbacks_received.each do |feedback| %>

            <div class="col-sm-12">
                <div class="card">
                    <div class="card-body">
                        <a href="<%= feedback_path(feedback.id) %>" class="card-title">
                        <!-- sauf pour le current -->
                        <p> Reçu le <%= get_time_verbose(feedback.created_at)%> - <%= feedback.sender.first_name %></p>
                        </a>
                    </div>
                </div>
            </div>

            <% end %>
            <p class="mb-0"></p>
        </div>
    </div>
</div>
